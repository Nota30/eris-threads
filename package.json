  },
  "author": "Nota30",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nota30/eris-threads.git"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-canonical": "^35.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "eris": "^0.17.0"
  }
}
